<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="407.0" prefWidth="518.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demojavafx.ParameterController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label text="Configuración" textAlignment="CENTER" textFill="#078e66">
    <font>
      <Font name="Baskerville Old Face" size="30.0" />
    </font>
  </Label>
  <TabPane prefHeight="433.0" prefWidth="478.0" tabClosingPolicy="UNAVAILABLE">
    <tabs>
      <Tab text="Tablero">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="246.0" prefWidth="478.0" />
        </content>
      </Tab>
         <Tab text="Individuos">
            <content>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="225.0" prefWidth="478.0">
                  <children>
                      <GridPane layoutY="20.0" prefHeight="198.0" prefWidth="478.0">
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.99998474121094" minWidth="10.0"
                                                 prefWidth="189.2"/>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="314.00001525878906" minWidth="10.0"
                                                 prefWidth="289.2"/>
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                          </rowConstraints>
                          <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="239.0"
                                 text="Turnos de vida iniciales:" GridPane.hgrow="ALWAYS">
                             <font>
                                 <Font name="Bookman Old Style Italic" size="12.0"/>
                             </font>
                          </Label>
                          <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="189.0"
                                 text="Probabilidad de reproducción:" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
                             <font>
                                 <Font name="Bookman Old Style Italic" size="12.0"/>
                             </font>
                          </Label>
                          <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="193.0"
                                 text="Probabilidad de clonación:" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
                             <font>
                                 <Font name="Bookman Old Style Italic" size="12.0"/>
                             </font>
                          </Label>
                          <Spinner fx:id="TurnosVidaInicialesSpinner" prefHeight="26.0" prefWidth="230.0"
                                   GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
                          <Slider fx:id="ProbReproIndividuoSlider" majorTickUnit="10.0" maxWidth="-Infinity"
                                  minorTickCount="2" prefHeight="14.0" prefWidth="230.0" showTickLabels="true"
                                  showTickMarks="true" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                                  GridPane.rowIndex="1" GridPane.valignment="CENTER">
                             <GridPane.margin>
                                 <Insets top="7.0"/>
                             </GridPane.margin>
                          </Slider>
                          <Slider fx:id="ProbClonIndividuoSlider" majorTickUnit="10.0" maxWidth="-Infinity"
                                  minorTickCount="2" prefHeight="14.0" prefWidth="230.0" showTickLabels="true"
                                  showTickMarks="true" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                                  GridPane.rowIndex="2" GridPane.valignment="CENTER">
                             <GridPane.margin>
                                 <Insets top="7.0"/>
                             </GridPane.margin>
                          </Slider>
                      </GridPane>
                  </children>
               </AnchorPane>
            </content>
         </Tab>
         <Tab text="Recursos" />
    </tabs>
  </TabPane>
  <HBox alignment="CENTER_RIGHT" prefHeight="105.0" prefWidth="478.0" spacing="3.0" style="-fx-border-width: 3px;">
    <children>
      <Button onAction="#onBotonGuardarClick" text="Guardar">
        <opaqueInsets>
          <Insets />
        </opaqueInsets></Button>
      <Button mnemonicParsing="false" onAction="#onBotonReiniciarClick" text="Reiniciar" />
      <Button mnemonicParsing="false" onAction="#onBotonCerrarClick" text="Cerrar" />
    </children>
    <VBox.margin>
      <Insets />
    </VBox.margin>
  </HBox>
</VBox>
